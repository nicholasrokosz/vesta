{
  "name": "vesta",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset && yarn db:seedRates && yarn db:seedReconciliation",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:seedRates": "dotenv -e .env.local -- tsx --tsconfig tsconfig.cli.json src/server/scripts/seed/seedRates.ts",
    "db:seedReconciliation": "dotenv -e .env.local -- tsx --tsconfig tsconfig.cli.json src/server/scripts/seed/seedReconciliation.ts",
    "dev": "concurrently -n NEXT,TS,ESLINT -c magenta,cyan,yellow \"next dev\" \"yarn ts --watch\" \"yarn eslint\"",
    "dev:worker": "dotenv -e .env.local -- tsx --tsconfig tsconfig.cli.json src/server/scripts/worker.ts",
    "dev:processScheduledMessages": "dotenv -e .env.local -- yarn start:processScheduledMessages",
    "dev:processReservationAvailability": "dotenv -e .env.local -- yarn start:processReservationAvailability",
    "dev:processGetMessageThreads": "dotenv -e .env.local -- yarn start:processGetMessageThreads",
    "dev:processGetPlaidTransactions": "dotenv -e .env.local -- yarn start:processGetPlaidTransactions",
    "dev:sentryTest": "dotenv -e .env.local -- tsx --tsconfig tsconfig.cli.json src/server/scripts/sentryTest.ts",
    "dev:twilioTest": "dotenv -e .env.local -- tsx --tsconfig tsconfig.cli.json src/server/scripts/twilioTest.ts",
    "dev:priceLabsTest": "dotenv -e .env.local -- tsx --tsconfig tsconfig.cli.json src/server/scripts/priceLabsTest.ts",
    "eslint": "esw -w src/**/*.{ts,tsx}",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "start": "next start",
    "start:worker": "tsx --tsconfig tsconfig.cli.json src/server/scripts/worker.ts",
    "start:processScheduledMessages": "tsx --tsconfig tsconfig.cli.json src/server/scripts/processScheduledMessages.ts",
    "start:processReservationAvailability": "tsx --tsconfig tsconfig.cli.json src/server/scripts/processReservationAvailability.ts",
    "start:processGetMessageThreads": "tsx --tsconfig tsconfig.cli.json src/server/scripts/processGetMessageThreads.ts",
    "start:processGetPlaidTransactions": "tsx --tsconfig tsconfig.cli.json src/server/scripts/processGetPlaidTransactions.ts",
    "test": "jest --watch src",
    "test:integration": "playwright test",
    "test:integration:ui": "playwright test --ui",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "ngrok": "ngrok http 3000"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --transpile-only prisma/seeds/seed.ts"
  },
